{
	"4 Input XOR Gate",
	"41H,"
	A	B	C	D	Q
	ZE	ZE	ZE	ZE	ZE
	ZE	ZE	ZE	ON	ON
	ZE	ZE	ON	ZE	ON
	ZE	ZE	ON	ON	ZE
	ZE	ON	ZE	ZE	ON
	ZE	ON	ZE	ON	ZE
	ZE	ON	ON	ZE	ZE
	ZE	ON	ON	ON	ON
	ON	ZE	ZE	ZE	ON
	ON	ZE	ZE	ON	ZE
	ON	ZE	ON	ZE	ZE
	ON	ZE	ON	ON	ON
	ON	ON	ZE	ZE	ZE
	ON	ON	ZE	ON	ON
	ON	ON	ON	ZE	ON
	ON	ON	ON	ON	ZE
	,
	[](const Widget::DrawArgs &args, Vec size) {
		nvgStrokeColor(args.vg, SUBLIGHTBLUE);
		nvgBeginPath(args.vg);
		drawInput1of4(args.vg, 33);
		drawInput2of4(args.vg, 35);
		drawInput3of4(args.vg, 35);
		drawInput4of4(args.vg, 33);
		drawXor(args.vg);
		drawOutput(args.vg, 59);
		nvgStrokeWidth(args.vg, 2);
		nvgStroke(args.vg);
	},
	LAMBDA_HEADER {
		return a ^ b ^ c ^ d; 
	}
}
